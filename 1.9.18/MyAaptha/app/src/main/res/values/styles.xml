    <style name="MyTheme" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarStyle">@style/MyActionBar</item>
    </style>

    <style name="MyActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:titleTextStyle">@style/TitleBarTextColor</item>
        <item name="android:background">#ffff</item>
    </style>

    <style name="TitleBarTextColor" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">#000000</item>
    </style>




    <style name="Buttoninstatheme" parent="android:Widget.Holo.Light.Button">
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="MyActionButtonOverflow" parent="android:style/Widget.Holo.Light.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_more_vert</item>
        <item name="android:background">?android:attr/actionBarItemBackground</item>
        <item name="android:contentDescription">"Lala"</item>
    </style>




    <style name="text_date">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <!--<item name="android:paddingLeft">5dp</item>-->
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/font_size</item>
        <item name="android:typeface">sans</item>
    </style>
    <style name="LVViewText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:textColor">#3399FF</item>
        <item name="android:textSize">@dimen/font_size</item>
        <item name="android:typeface">sans</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingTop">5dp</item>
    </style>
    <style name="userUpdateButton">
        <item name="android:layout_width">@dimen/editTextHeight</item>
        <item name="android:layout_height">@dimen/editTextHeight</item>
        <item name="android:text">@string/fai_edit</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">5dp</item>
        <!--<item name="android:textSize">@dimen/faiIconsize</item>-->
        <item name="android:background">@drawable/circle_icon</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>



    <style name="userFileButton">
        <item name="android:layout_width">@dimen/editTextHeight</item>
        <item name="android:layout_height">@dimen/editTextHeight</item>
        <item name="android:text">@string/fai_file</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">5dp</item>
      <!--  <item name="android:textSize">@dimen/faiIconsize</item>-->
        <item name="android:background">@drawable/circle_icon</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="userDeleteButton">
        <item name="android:layout_width">@dimen/editTextHeight</item>
        <item name="android:layout_height">@dimen/editTextHeight</item>
        <item name="android:text">@string/fai_delete</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">5dp</item>
        <!--<item name="android:textSize">@dimen/faiIconsize</item>-->
        <item name="android:background">@drawable/circle_icon</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="submitspinner" parent="@android:style/TextAppearance.Widget.TextView.SpinnerItem">
        <!-- <item name="android:layout_width">match_parent</item>
         <item name="android:layout_height">match_parent</item>
         <item name="android:textSize">25sp</item>-->

        <item name="android:singleLine">true</item>
        <item name="android:gravity">center</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/spinnerHeight</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:drawSelectorOnTop">true</item>
    </style>
    <style name="EditTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/editTextHeight</item>
        <item name="android:gravity">start|center_vertical</item>
        <!--<item name="android:textColor">@color/TextColorDark</item>-->
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingLeft">@dimen/padding_x2</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">@dimen/font_size</item>
        <item name="android:background">@drawable/form_edit_text_bg</item>
        <item name="android:typeface">sans</item>
        <item name="android:layout_margin">@dimen/cardElevation</item>
    </style>
    <style name="mainBlockButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">@dimen/view_width</item>
        <item name="android:layout_margin">@dimen/view_width</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/font_size</item>
    </style>
    <style name="faiStyles">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">@dimen/font_size</item>
        <!--<item name="android:background">@color/gray_ccc</item>-->
    </style>
    <style name="TextAppearance.App.TextInputLayout" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="LayoutHeadings">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/layoutHeadings</item>
        <item name="android:padding">1dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/insta_blue_bg</item>
        <item name="android:gravity">center</item>
        <item name="android:typeface">sans</item>
    </style>
    <style name="backfrag_button_submit">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/back_icon</item>
        <item name="android:gravity">right</item>
        <!--<item name="android:layout_marginLeft">5dp</item>-->
        <!--<item name="android:layout_marginRight">5dp</item>-->
        <item name="android:padding">@dimen/padding_x2</item>
    </style>
    <style name="ll_date">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginLeft">@dimen/padding_x2</item>
        <item name="android:layout_marginRight">@dimen/padding_x2</item>
        <item name="android:layout_marginTop">@dimen/padding_x2</item>
        <!--<item name="android:textSize">14dp</item>-->
        <item name="android:layout_marginBottom">3dp</item>
        <!--<item name="android:typeface">sans</item>-->
    </style>
    <style name="NewDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/transparent90</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
    </style>
    <style name="date_field">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/spinnerHeight</item>
        <item name="android:background">@drawable/form_edit_text_bg</item>
        <item name="android:hint">@string/selDate</item>
        <item name="android:inputType">none</item>
        <item name="android:editable">false</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingLeft">@dimen/padding_x2</item>
        <item name="android:textSize">@dimen/font_size</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:drawableRight">@drawable/calendar</item>
        <item name="android:drawablePadding">@dimen/cardElevation</item>
        <item name="android:typeface">sans</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_margin">@dimen/cardElevation</item>
    </style>
    <style name="TextInputLayoutStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/editTextHeight</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">#00000000</item>
        <item name="android:textSize">@dimen/font_size</item>
        <item name="android:gravity">start</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingLeft">@dimen/padding_x2</item>
        <item name="android:paddingRight">@dimen/padding_x2</item>
        <item name="android:typeface">sans</item>
        <item name="android:focusable">true</item>
    </style>
    <style name="TextviewStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="textViewStyle">
        <item name="android:textSize">@dimen/font_size</item>
       <item name="android:gravity">start|center_vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <!--<item name="android:textColor">@color/black</item>-->
        <item name="android:paddingLeft">@dimen/padding_x2</item>
        <item name="android:typeface">sans</item>
    </style>
    <style name="MyRadioButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <!--<item name="android:paddingRight">10dp</item>-->
        <item name="colorControlNormal">@color/gray</item>
        <item name="colorControlActivated">@color/insta_blue_bg</item>
    </style>
    <style name="maleRadioButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawableLeft">@drawable/male</item>
        <item name="android:text">Male</item>
        <item name="android:padding">0dp</item>
        <item name="android:textSize">@dimen/font_sizedesc</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/gray</item>
    </style>
    <style name="feMaleRadioButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawableLeft">@drawable/female</item>
        <item name="android:text">Female</item>
        <item name="android:textSize">@dimen/font_sizedesc</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/gray</item>
    </style>
    <style name="otherRadioButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawableLeft">@drawable/female</item>
        <item name="android:text">Transgender</item>
        <item name="android:textSize">@dimen/font_sizedesc</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/gray</item>
    </style>
    <style name="personalRadioButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:drawableLeft">@drawable/personalimg</item>
        <item name="android:text">@string/personal</item>
        <item name="android:textSize">@dimen/font_sizedesc</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/gray</item>
    </style>
    <style name="publicRadioButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:drawableLeft">@drawable/publicimg</item>
        <item name="android:text">@string/Public</item>
        <item name="android:textSize">@dimen/font_sizedesc</item>
        <item name="android:textStyle">bold</item>
        <item name="android:checked">true</item>
        <item name="android:textColor">@color/gray</item>
    </style>
    <style name="friendsRadioButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:drawableLeft">@drawable/friendsimg</item>
        <item name="android:text">@string/friends</item>
        <item name="android:textSize">@dimen/font_sizedesc</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/gray</item>
    </style>
    <style name="relativesRadioButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:drawableLeft">@drawable/relativesimg</item>
        <item name="android:text">@string/relatives</item>
        <item name="android:textSize">@dimen/font_sizedesc</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/gray</item>
    </style>

    <style name="SpinnerStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/spinnerHeight</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:textSize">@dimen/font_size</item>
        <item name="android:padding">0dp</item>
    </style>

    <style name="cust_button">
        <!--<item name="android:textSize">@dimen/cust_button_textsize</item>-->
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/TextColorLight</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">0.0dip</item>
        <item name="android:shadowColor">@color/TextColorDark</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">4.0</item>
    </style>


</resources>
